{
  "manifest_version": 2,
  "name": "general strike",
  "author": "cass spain",
  "version": "0.1",
  "description": "There may be no ethical consumption under late capitalism, but it doesn't hurt to try.",
  "browser_action": {
    "default_icon": "Bells.png"
  },
  "icons": {
    "16": "Bells.png",
    "48": "Bells.png",
    "128": "Bells.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/jquery-3.4.1.slim.js", "scripts/content.js"]
    }
  ],
  "background": {
    "scripts": ["scripts/background.js"]
  }
}
