{
  "manifest_version": 2,
  "name": "strikethru",
  "author": "cass spain",
  "version": "0.1",
  "description": "There may be no ethical consumption under late capitalism, but it doesn't hurt to try.",
  "browser_action": {
    "default_icon": "Bells.png",
    "default_popup": "index.html",
    "default_title": "Open the popup!"
  },
  "icons": {
    "16": "Bells.png",
    "48": "Bells.png",
    "128": "Bells.png"
  },
  "permissions": ["tabs", "storage"],
  "web_accessible_resources": ["*.html"]
}
